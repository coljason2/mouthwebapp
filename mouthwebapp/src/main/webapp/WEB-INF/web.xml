<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>about</servlet-name>
		<servlet-class>urlpattern.about</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>about</servlet-name>
		<url-pattern>/about</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>airpollution</servlet-name>
		<servlet-class>urlpattern.airpollution</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>airpollution</servlet-name>
		<url-pattern>/airpollution</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>goldprice</servlet-name>
		<servlet-class>urlpattern.goldprice</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>goldprice</servlet-name>
		<url-pattern>/goldprice</url-pattern>
	</servlet-mapping>
	<!-- welcome -->
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<!-- error -->
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/pages/error/error.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/pages/error/error.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/pages/error/error.jsp</location>
	</error-page>
</web-app>
